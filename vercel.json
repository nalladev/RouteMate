{
  "buildCommand": "npx expo export --platform web",
  "outputDirectory": "dist/client",
  "installCommand": "pnpm install",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.2.21",
      "includeFiles": "dist/server/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}